{
  "name": "bizzwords",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:gh-pages": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e": "playwright test",
    "e2e:safe": "firebase emulators:exec --only auth,firestore --project demo-bizzwords \"npm run e2e\"",
    "serve:ssr:bizzwords": "node dist/bizzwords/server/server.mjs",
    "deploy:ghpages": "ng deploy --base-href=/bizzwords/",
    "apply-freemium-rules": "node scripts/apply-freemium-rules.mjs",
    "android:icons": "node scripts/copy-android-icons.mjs",
    "cap:sync": "ng build && npx cap sync",
    "cap:open": "npx cap open android"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "packageManager": "npm@11.6.4",
  "dependencies": {
    "@angular/common": "^21.0.3",
    "@angular/compiler": "^21.0.3",
    "@angular/core": "^21.0.3",
    "@angular/fire": "^20.0.1",
    "@angular/forms": "^21.0.3",
    "@angular/platform-browser": "^21.0.3",
    "@angular/platform-server": "^21.0.3",
    "@angular/router": "^21.0.3",
    "@angular/service-worker": "^21.0.3",
    "@angular/ssr": "^21.0.2",
    "@capacitor/android": "^8.0.0",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^8.0.0",
    "@capacitor/preferences": "^8.0.0",
    "@huggingface/transformers": "^3.8.1",
    "@ngneat/tailwind": "^7.0.3",
    "@revenuecat/purchases-capacitor": "^11.3.0",
    "@tailwindcss/postcss": "^4.1.17",
    "execa": "^9.6.1",
    "express": "^5.2.1",
    "firebase": "^12.6.0",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular/build": "^21.0.2",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.3",
    "@angular/platform-browser-dynamic": "^21.0.3",
    "@capacitor/assets": "^3.0.5",
    "@playwright/test": "^1.57.0",
    "@types/express": "^5.0.6",
    "@types/node": "^20.17.19",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/web-worker": "^4.0.15",
    "@zenai/playwright-coding-agent-reporter": "^1.2.1",
    "angular-cli-ghpages": "^2.0.3",
    "gh-pages": "^6.3.0",
    "jsdom": "^27.3.0",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "vitest": "^4.0.15",
    "vitest-llm-reporter": "^1.2.1"
  }
}
