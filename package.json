{
  "name": "bizzwords",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:gh-pages": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:bizzwords": "node dist/bizzwords/server/server.mjs",
    "deploy:ghpages": "ng deploy --base-href=/bizzwords/"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/common": "^21.0.1",
    "@angular/compiler": "^21.0.1",
    "@angular/core": "^21.0.1",
    "@angular/forms": "^21.0.1",
    "@angular/platform-browser": "^21.0.1",
    "@angular/platform-server": "^21.0.1",
    "@angular/router": "^21.0.1",
    "@angular/service-worker": "^21.0.1",
    "@angular/ssr": "^21.0.1",
    "@huggingface/transformers": "^3.8.0",
    "@ngneat/tailwind": "^7.0.3",
    "@tailwindcss/postcss": "^4.1.17",
    "express": "^5.1.0",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.1",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "^21.0.1",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.19",
    "@vitest/coverage-v8": "^4.0.14",
    "angular-cli-ghpages": "^2.0.3",
    "gh-pages": "^6.3.0",
    "jsdom": "^27.1.0",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
